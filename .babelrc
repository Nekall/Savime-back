{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": "commonjs",
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "ignore": [
        "node_modules"
    ],
    "sourceType": "unambiguous",
    "inputSourceMap": false,
    "sourceMaps": false,
    "only": [
        "*.js"
    ]
}
